
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Auth {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Project {
  project_id          Int     @id @default(autoincrement())
  name      String
  body      String
  user_id   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}